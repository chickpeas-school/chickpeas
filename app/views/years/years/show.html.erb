<p id="notice"><%= notice %></p>

<p>
  <strong>Year:</strong>
  <%= @year.value %>
</p>

<p>
  <strong>Is Current Year?</strong>
  <%= @year.current_year %>
</p>

<hr />
<p><strong>Parents</strong></p>
<% @year.parents.each do |parent| %>
	<p><%= link_to parent.name, parent_path(parent) %></p>
<% end %>

<%= link_to 'Edit', edit_years_year_path(@year) %> |
<%= link_to 'Back', years_years_path %>
